<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".result"
    android:background="#000000">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="324dp"
            android:layout_height="184dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="19dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_result" />

        <TextView
            android:id="@+id/result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="43dp"
            android:layout_marginTop="150dp"
            android:fontFamily="@font/bungee"
            android:text="RESULT"
            android:textColor="#0066ff"
            android:textSize="30dp" />

        <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/result"
        android:textColor="#ffffff"
        android:layout_alignStart="@+id/result"
        android:layout_marginStart="1dp"
        android:layout_marginTop="32dp"
        android:text="Buy Rate :"
        android:fontFamily="@font/rubik_medium"
        android:textSize="20dp"/>

         <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/result"
            android:layout_marginStart="47dp"
            android:layout_marginTop="29dp"
            android:layout_toEndOf="@+id/textView5"
            android:fontFamily="@font/rubik_medium"
            android:text="Enter Details"
            android:textColor="#0066ff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView5"
            android:layout_alignStart="@+id/textView5"
            android:layout_marginStart="1dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/rubik_medium"
            android:text="Sell Rate :"
            android:textColor="#ffffff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView10"
            android:layout_marginStart="47dp"
            android:layout_marginTop="29dp"
            android:layout_toEndOf="@+id/textView6"
            android:fontFamily="@font/rubik_medium"
            android:text="Enter Details"
            android:textColor="#0066ff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_alignStart="@+id/textView6"
            android:layout_marginStart="1dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/rubik_medium"
            android:text="Lot:"
            android:textColor="#ffffff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView11"
            android:layout_marginStart="47dp"
            android:layout_marginTop="34dp"
            android:layout_toEndOf="@+id/textView7"
            android:fontFamily="@font/rubik_medium"
            android:text="Enter Details"
            android:textColor="#0066ff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_alignStart="@+id/textView7"
            android:layout_marginStart="1dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/rubik_medium"
            android:text="Turnover Charges :"
            android:textColor="#ffffff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView12"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:layout_toEndOf="@+id/textView8"
            android:fontFamily="@font/rubik_medium"
            android:text="Enter Details"
            android:textColor="#0066ff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView8"
            android:layout_alignStart="@+id/textView8"
            android:layout_marginStart="1dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/rubik_medium"
            android:text="Profit / Loss :"
            android:textColor="#ffffff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView13"
            android:layout_marginStart="47dp"
            android:layout_marginTop="33dp"
            android:layout_toEndOf="@+id/textView9"
            android:fontFamily="@font/rubik_medium"
            android:text="Enter Details"
            android:textColor="#0066ff"
            android:textSize="20dp" />

        <Button
            android:id="@+id/courses"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:text="Courses"
            android:textColor="#ffffff"
            android:fontFamily="@font/rubik_medium"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="21dp"
            android:background="@drawable/login2button" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>